#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char s[100];
    int i, n;
    gets(s);
    n=strlen(s);
    for(i=n-1; i>=0; i--) 
        printf("%c", s[i]);
    return 0;
}
